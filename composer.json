{
    "name": "bolechen/media-manager",
    "description": "fork from laravel-admin，修复了一些 bug，自用，有需要请看源项目",
    "type": "library",
    "keywords": ["laravel-admin", "media", "manager"],
    "homepage": "https://github.com/bolechen/media-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.5",
        "bolechen/laravel-admin": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\Media\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\Media\\MediaServiceProvider"
            ]

        }
    }
}
